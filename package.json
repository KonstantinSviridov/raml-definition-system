    "definititions",
    "typesystem"
  ],
  "files": [
    "dist/",
    "typings.json"
  ],
  "homepage": "https://github.com/mulesoft-labs/typesystem-ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mulesoft-labs/typesystem-ts/issues"
  },
